newData.hasChildren(['red', 'green', 'blue'])
    && newData.child('red').isNumber()
    && newData.child('green').isNumber()
    && newData.child('blue').isNumber()


"red": {
  ".validate": "newData.isNumber() && newData.val() >= 0 && newData.val() <= 255"
},
"green": {
  ".validate": "newData.isNumber() && newData.val() >= 0 && newData.val() <= 255"
},
"blue": {
  ".validate": "newData.isNumber() && newData.val() >= 0 && newData.val() <= 255"
}
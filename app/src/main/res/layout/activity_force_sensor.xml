<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context="com.madebyatomicrobot.walker.robot.ForceSensorActivity">

    <data class=".ForceSensorBinding">
    </data>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <SeekBar
            android:id="@+id/fsr1_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:max="2047"
            android:progress="0"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/fsr1_1Label"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"/>

        <SeekBar
            android:id="@+id/fsr1_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:max="2047"
            android:progress="0"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/fsr1_2Label"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fsr1_1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"/>

        <SeekBar
            android:id="@+id/fsr1_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:max="2047"
            android:progress="0"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/fsr1_3Label"
            app:layout_constraintRight_toRightOf="@+id/fsr1_2"
            app:layout_constraintTop_toBottomOf="@+id/fsr1_2"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"/>

        <SeekBar
            android:id="@+id/fsr1_4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:max="2047"
            android:progress="0"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/fsr1_4Label"
            app:layout_constraintRight_toRightOf="@+id/fsr1_3"
            app:layout_constraintTop_toBottomOf="@+id/fsr1_3"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"/>

        <TextView
            android:id="@+id/fsr1_1Label"
            style="?android:textAppearanceMedium"
            android:layout_width="128dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="1 / FSR1"
            app:layout_constraintBottom_toBottomOf="@+id/fsr1_1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fsr1_1"/>

        <TextView
            android:id="@+id/fsr1_2Label"
            style="?android:textAppearanceMedium"
            android:layout_width="128dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="1 / FSR2"
            app:layout_constraintBottom_toBottomOf="@+id/fsr1_2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fsr1_2"
            app:layout_constraintVertical_bias="1.0"/>

        <TextView
            android:id="@+id/fsr1_3Label"
            style="?android:textAppearanceMedium"
            android:layout_width="128dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="1 / FSR3"
            app:layout_constraintBottom_toBottomOf="@+id/fsr1_3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fsr1_3"
            app:layout_constraintVertical_bias="1.0"/>

        <TextView
            android:id="@+id/fsr1_4Label"
            style="?android:textAppearanceMedium"
            android:layout_width="128dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="1 / FSR4"
            app:layout_constraintBottom_toBottomOf="@+id/fsr1_4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fsr1_4"
            app:layout_constraintVertical_bias="1.0"/>

        <TextView
            android:id="@+id/angle1"
            style="?android:textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="32dp"
            android:text="1 / Angle"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fsr1_4"/>

        <TextView
            android:id="@+id/magnitude1"
            style="?android:textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="1 / Magnitude"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angle1"/>

    </android.support.constraint.ConstraintLayout>

</layout>